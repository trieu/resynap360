{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "TrackEventModel",
  "type": "object",
  "required": [
    "tenant_id",
    "mediahost",
    "schema_version",
    "metric",
    "tpurl"
  ],
  "properties": {
    "schema_version": {
      "type": "string",
      "pattern": "^\\d{4}\\.\\d{2}\\.\\d{2}$"
    },
    "event_id": {
      "type": ["string", "null"],
      "format": "uuid"
    },
    "tenant_id": {
      "type": "string"
    },
    "observer_id": {
     "type": ["string", "null"]
    },
    "datetime": {
      "type": "string",
      "format": "date-time"
    },
    "unix_timestamp": {
      "type": "number"
    },
    "metric": {
      "type": "string"
    },
    "visid": {
      "type": ["string", "null"],
      "format": "uuid"
    },
    "mediahost": {
      "type": "string"
    },
    "tpurl": {
      "type": ["string", "null"],
      "format": "uri"
    },
    "tprefurl": {
      "type": ["string", "null"]
    },
    "tprefdomain": {
      "type": ["string", "null"]
    },
    "tpname": {
      "type": ["string", "null"]
    },
    "utmdata": {
      "type": "object",
      "properties": {
        "utmsource": { "type": "string" },
        "utmmedium": { "type": "string" },
        "utmcampaign": { "type": "string" },
        "utmterm": { "type": "string" },
        "utmcontent": { "type": "string" }
      },
      "additionalProperties": true
    },
    "fgp": {
      "type": "string"
    },
    "sessionid": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "platform": {
      "type": "string"
    },
    "user_agent": {
      "type": "string"
    },
    "app_version": {
      "type": "string"
    },
    "device": {
      "type": "string"
    },
    "open_in_app": {
      "type": "string"
    },
    "is_in_app_browser": {
      "type": "boolean"
    },
    "event_payload": {
      "type": "object",
      "additionalProperties": true
    },
    "profile_traits": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}